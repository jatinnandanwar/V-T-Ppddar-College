<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.college_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.College_App"
        tools:targetApi="31">
        <activity
            android:name=".Holiday"
            android:exported="false" />
        <activity
            android:name=".TimeTable"
            android:exported="false" />
        <activity
            android:name=".Exam_Schedule"
            android:exported="false" />
        <activity
            android:name=".Learning_Material"
            android:exported="false" />
        <activity
            android:name=".CourseDetails"
            android:exported="false" />
        <activity
            android:name=".FacultyDetails"
            android:exported="false" />
        <activity
            android:name=".FullScreenImage"
            android:exported="false" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".splash"
            android:exported="true"
            android:theme="@style/splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>